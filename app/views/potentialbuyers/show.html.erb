<p id="notice"><%= notice %></p>
<table>
  <thead>
  <tr>
    <th>User Name</th>
    <th>Email ID</th>
    <th>Company ID</th>
    <th>Contact</th>
    <th>Preferred Contact</th>
    <th colspan="3"></th>
  </tr>
  </thead>
<% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.company_id %></td>
      <td><%= user.contactinfo %></td>
      <td><%= user.preferredcontact %></td>
    </tr>
<% end %>
